   ],
   "source": [
    "def solution(word) : \n",
    "    answer = \"\"\n",
    "    for i in range(len(word)) : \n",
    "        if word.find(word[i]) == i : \n",
    "            answer += word[i]\n",
    "    return answer\n",
    "\n",
    "solution(\"sssssttessskssee\")"
   ]
  }
