   "source": [
    "edge = [[1, 2, 2], [1, 3, 4], [2, 5, 5], [3, 4, 5], [4, 2, 2]]\n",
    "n = len(edge)\n",
    "graph = [[0]*(n+1) for _ in range(n+1)]\n",
    "for a,b,c in edge : \n",
    "    graph[a][b] = c\n",
    "\n",
    "graph"
   ]
